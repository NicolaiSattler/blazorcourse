{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Yarp": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "RestApiRoute": {
        "ClusterId": "RestApi",
        "Match": {
          "Path": "/api/photo/{*any}"
        }
      },
      "GrpcRoute": {
        "ClusterId": "Grpc",
        "Match": {
          "Path": "/comment/commenter/{*any}"
        }
      }
    },
    "Clusters": {
      "RestApi": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:7078"
          }
        }
      },

      "Grpc": {
        "Destinations": {
          "destination1": {
            //Helaas werkt https niet op macos?
            "Address": "http://localhost:5105"
          }
        }
      }
    }
  }
}